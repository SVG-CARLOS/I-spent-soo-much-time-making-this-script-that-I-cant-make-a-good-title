-- CREATE THE UI
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
wait(1)
local UI, NotifyFunc = CreateAdvancedUI()

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- FPS COUNTER (bonus feature)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
spawn(function()
    local fpsGui = Instance.new("ScreenGui", player1.PlayerGui)
    fpsGui.Name = "FPSCounter"
    fpsGui.ResetOnSpawn = false

    local fpsLabel = Instance.new("TextLabel", fpsGui)
    fpsLabel.Size = UDim2.new(0, 100, 0, 25)
    fpsLabel.Position = UDim2.new(0, 10, 0, 10)
    fpsLabel.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
    fpsLabel.BackgroundTransparency = 0.3
    fpsLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
    fpsLabel.Font = Enum.Font.GothamBold
    fpsLabel.TextSize = 14
    fpsLabel.Text = "FPS: --"
    Instance.new("UICorner", fpsLabel).CornerRadius = UDim.new(0, 6)
    Instance.new("UIStroke", fpsLabel).Color = Color3.fromRGB(0, 100, 50)

    local lastTick = tick()
    local frameCount = 0

    RunService.RenderStepped:Connect(function()
        frameCount = frameCount + 1
        if tick() - lastTick >= 1 then
            fpsLabel.Text = "FPS: " .. frameCount
            if frameCount >= 50 then
                fpsLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
            elseif frameCount >= 30 then
                fpsLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
            else
                fpsLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            end
            frameCount = 0
            lastTick = tick()
        end
    end)
end)

wait(9e9)
