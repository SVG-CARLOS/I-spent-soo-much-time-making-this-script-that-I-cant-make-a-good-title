-- SECTION 3: NOTIFICATION SYSTEM
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local NotifGui = Instance.new("ScreenGui")
NotifGui.Name = "NotificationSystem"
NotifGui.ResetOnSpawn = false
NotifGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
NotifGui.Parent = player1:WaitForChild("PlayerGui")

local NotifHolder = Instance.new("Frame")
NotifHolder.Name = "NotifHolder"
NotifHolder.BackgroundTransparency = 1
NotifHolder.Position = UDim2.new(1, -320, 0, 10)
NotifHolder.Size = UDim2.new(0, 310, 1, -20)
NotifHolder.Parent = NotifGui

local NotifLayout = Instance.new("UIListLayout")
NotifLayout.Padding = UDim.new(0, 6)
NotifLayout.SortOrder = Enum.SortOrder.LayoutOrder
NotifLayout.VerticalAlignment = Enum.VerticalAlignment.Top
NotifLayout.Parent = NotifHolder

local function Notify(title, text, duration, color)
    duration = duration or 4
    color = color or Color3.fromRGB(88, 101, 242)
    
    local NotifFrame = Instance.new("Frame")
    NotifFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    NotifFrame.BorderSizePixel = 0
    NotifFrame.Size = UDim2.new(1, 0, 0, 0)
    NotifFrame.ClipsDescendants = true
    NotifFrame.Parent = NotifHolder
    
    local NotifCorner = Instance.new("UICorner", NotifFrame)
    NotifCorner.CornerRadius = UDim.new(0, 8)
    
    local AccentBar = Instance.new("Frame", NotifFrame)
    AccentBar.BackgroundColor3 = color
    AccentBar.BorderSizePixel = 0
    AccentBar.Size = UDim2.new(0, 3, 1, 0)
    
    local TitleLabel = Instance.new("TextLabel", NotifFrame)
    TitleLabel.BackgroundTransparency = 1
    TitleLabel.Position = UDim2.new(0, 14, 0, 6)
    TitleLabel.Size = UDim2.new(1, -20, 0, 18)
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.Text = title
    TitleLabel.TextColor3 = color
    TitleLabel.TextSize = 14
    TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    local BodyLabel = Instance.new("TextLabel", NotifFrame)
    BodyLabel.BackgroundTransparency = 1
    BodyLabel.Position = UDim2.new(0, 14, 0, 26)
    BodyLabel.Size = UDim2.new(1, -20, 0, 30)
    BodyLabel.Font = Enum.Font.Gotham
    BodyLabel.Text = text
    BodyLabel.TextColor3 = Color3.fromRGB(200, 200, 210)
    BodyLabel.TextSize = 12
    BodyLabel.TextXAlignment = Enum.TextXAlignment.Left
    BodyLabel.TextWrapped = true
    
    local ProgressBar = Instance.new("Frame", NotifFrame)
    ProgressBar.BackgroundColor3 = color
    ProgressBar.BorderSizePixel = 0
    ProgressBar.Position = UDim2.new(0, 0, 1, -2)
    ProgressBar.Size = UDim2.new(1, 0, 0, 2)
    
    -- Animate in
    TweenService:Create(NotifFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(1, 0, 0, 62)
    }):Play()
    
    -- Progress bar countdown
    TweenService:Create(ProgressBar, TweenInfo.new(duration, Enum.EasingStyle.Linear), {
        Size = UDim2.new(0, 0, 0, 2)
    }):Play()
    
    task.delay(duration, function()
        TweenService:Create(NotifFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(1, 0, 0, 0)
        }):Play()
        task.wait(0.35)
        NotifFrame:Destroy()
    end)
end

Notify("âš¡ Reanimate v3.0", "Script is loading... Please wait.", 3, Color3.fromRGB(88, 101, 242))

