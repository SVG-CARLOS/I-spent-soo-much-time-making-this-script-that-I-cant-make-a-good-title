-- Tab System
local TabBar = Instance.new("Frame", MainFrame)
TabBar.Name = "TabBar"
TabBar.BackgroundColor3 = Color3.fromRGB(18, 18, 30)
TabBar.BorderSizePixel = 0
TabBar.Position = UDim2.new(0, 0, 0, 40)
TabBar.Size = UDim2.new(1, 0, 0, 35)

local TabLayout = Instance.new("UIListLayout", TabBar)
TabLayout.FillDirection = Enum.FillDirection.Horizontal
TabLayout.SortOrder = Enum.SortOrder.LayoutOrder
TabLayout.Padding = UDim.new(0, 0)

local TabPages = Instance.new("Frame", MainFrame)
TabPages.Name = "TabPages"
TabPages.BackgroundTransparency = 1
TabPages.Position = UDim2.new(0, 0, 0, 75)
TabPages.Size = UDim2.new(1, 0, 1, -75)

local ActiveTab = nil
local TabButtons = {}
local Pages = {}

local tabNames = {"Main", "Player", "Visual", "Misc"}

local function SwitchTab(tabName)
    ActiveTab = tabName
    for name, btn in pairs(TabButtons) do
        if name == tabName then
            TweenService:Create(btn, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(88, 101, 242),
                TextColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
        else
            TweenService:Create(btn, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(18, 18, 30),
                TextColor3 = Color3.fromRGB(120, 120, 140)
            }):Play()
        end
    end
    for name, page in pairs(Pages) do
        page.Visible = (name == tabName)
    end
end

for i, tabName in ipairs(tabNames) do
    local TabBtn = Instance.new("TextButton", TabBar)
    TabBtn.Name = tabName
    TabBtn.BackgroundColor3 = Color3.fromRGB(18, 18, 30)
    TabBtn.BorderSizePixel = 0
    TabBtn.Size = UDim2.new(1 / #tabNames, 0, 1, 0)
    TabBtn.Font = Enum.Font.GothamBold
    TabBtn.Text = tabName
    TabBtn.TextColor3 = Color3.fromRGB(120, 120, 140)
    TabBtn.TextSize = 13
    TabBtn.LayoutOrder = i
    TabBtn.AutoButtonColor = false
    TabButtons[tabName] = TabBtn
    
    local Page = Instance.new("ScrollingFrame", TabPages)
    Page.Name = tabName
    Page.BackgroundTransparency = 1
    Page.Size = UDim2.new(1, 0, 1, 0)
    Page.ScrollBarThickness = 3
    Page.ScrollBarImageColor3 = Color3.fromRGB(88, 101, 242)
    Page.CanvasSize = UDim2.new(0, 0, 0, 0)
    Page.AutomaticCanvasSize = Enum.AutomaticSize.Y
    Page.Visible = false
    Pages[tabName] = Page
    
    local PageLayout = Instance.new("UIListLayout", Page)
    PageLayout.Padding = UDim.new(0, 5)
    PageLayout.SortOrder = Enum.SortOrder.LayoutOrder
    
    local PagePadding = Instance.new("UIPadding", Page)
    PagePadding.PaddingLeft = UDim.new(0, 10)
    PagePadding.PaddingRight = UDim.new(0, 10)
    PagePadding.PaddingTop = UDim.new(0, 8)
    PagePadding.PaddingBottom = UDim.new(0, 8)
    
    TabBtn.MouseButton1Click:Connect(function()
        SwitchTab(tabName)
    end)
end

-- UI Element Creators
local function CreateToggle(parent, text, default, keybind, callback)
    local order = #parent:GetChildren()
    
    local ToggleFrame = Instance.new("Frame", parent)
    ToggleFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    ToggleFrame.BorderSizePixel = 0
    ToggleFrame.Size = UDim2.new(1, 0, 0, 38)
    ToggleFrame.LayoutOrder = order
    Instance.new("UICorner", ToggleFrame).CornerRadius = UDim.new(0, 8)
    
    local ToggleLabel = Instance.new("TextLabel", ToggleFrame)
    ToggleLabel.BackgroundTransparency = 1
    ToggleLabel.Position = UDim2.new(0, 12, 0, 0)
    ToggleLabel.Size = UDim2.new(0.5, 0, 1, 0)
    ToggleLabel.Font = Enum.Font.Gotham
    ToggleLabel.Text = text
    ToggleLabel.TextColor3 = Color3.fromRGB(200, 200, 215)
    ToggleLabel.TextSize = 13
    ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    if keybind then
        local KeyLabel = Instance.new("TextLabel", ToggleFrame)
        KeyLabel.BackgroundTransparency = 1
        KeyLabel.Position = UDim2.new(0.5, 0, 0, 0)
        KeyLabel.Size = UDim2.new(0.2, 0, 1, 0)
        KeyLabel.Font = Enum.Font.GothamBold
        KeyLabel.Text = "[" .. keybind .. "]"
        KeyLabel.TextColor3 = Color3.fromRGB(80, 80, 100)
        KeyLabel.TextSize = 11
        KeyLabel.TextXAlignment = Enum.TextXAlignment.Right
    end
    
    local ToggleBtn = Instance.new("Frame", ToggleFrame)
    ToggleBtn.Name = "ToggleBtn"
    ToggleBtn.BackgroundColor3 = default and Color3.fromRGB(88, 101, 242) or Color3.fromRGB(50, 50, 65)
    ToggleBtn.Position = UDim2.new(1, -58, 0.5, -11)
    ToggleBtn.Size = UDim2.new(0, 44, 0, 22)
    Instance.new("UICorner", ToggleBtn).CornerRadius = UDim.new(1, 0)
    
    local ToggleCircle = Instance.new("Frame", ToggleBtn)
    ToggleCircle.Name = "Circle"
    ToggleCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleCircle.Position = default and UDim2.new(1, -20, 0.5, -8) or UDim2.new(0, 2, 0.5, -8)
    ToggleCircle.Size = UDim2.new(0, 16, 0, 16)
    Instance.new("UICorner", ToggleCircle).CornerRadius = UDim.new(1, 0)
    
    local Enabled = default or false
    
    local ClickBtn = Instance.new("TextButton", ToggleFrame)
    ClickBtn.BackgroundTransparency = 1
    ClickBtn.Size = UDim2.new(1, 0, 1, 0)
    ClickBtn.Text = ""
    ClickBtn.ZIndex = 5
    
    local function Toggle()
        Enabled = not Enabled
        TweenService:Create(ToggleBtn, TweenInfo.new(0.25), {
            BackgroundColor3 = Enabled and Color3.fromRGB(88, 101, 242) or Color3.fromRGB(50, 50, 65)
        }):Play()
        TweenService:Create(ToggleCircle, TweenInfo.new(0.25, Enum.EasingStyle.Back), {
            Position = Enabled and UDim2.new(1, -20, 0.5, -8) or UDim2.new(0, 2, 0.5, -8)
        }):Play()
        if callback then callback(Enabled) end
    end
    
    ClickBtn.MouseButton1Click:Connect(Toggle)
    
    return {Toggle = Toggle, SetState = function(state)
        if state ~= Enabled then Toggle() end
    end}
end

local function CreateSlider(parent, text, min, max, default, callback)
    local order = #parent:GetChildren()
    
    local SliderFrame = Instance.new("Frame", parent)
    SliderFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    SliderFrame.BorderSizePixel = 0
    SliderFrame.Size = UDim2.new(1, 0, 0, 50)
    SliderFrame.LayoutOrder = order
    Instance.new("UICorner", SliderFrame).CornerRadius = UDim.new(0, 8)
    
    local SliderLabel = Instance.new("TextLabel", SliderFrame)
    SliderLabel.BackgroundTransparency = 1
    SliderLabel.Position = UDim2.new(0, 12, 0, 2)
    SliderLabel.Size = UDim2.new(0.6, 0, 0, 20)
    SliderLabel.Font = Enum.Font.Gotham
    SliderLabel.Text = text
    SliderLabel.TextColor3 = Color3.fromRGB(200, 200, 215)
    SliderLabel.TextSize = 13
    SliderLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    local ValueLabel = Instance.new("TextLabel", SliderFrame)
    ValueLabel.BackgroundTransparency = 1
    ValueLabel.Position = UDim2.new(0.6, 0, 0, 2)
    ValueLabel.Size = UDim2.new(0.35, 0, 0, 20)
    ValueLabel.Font = Enum.Font.GothamBold
    ValueLabel.Text = tostring(default)
    ValueLabel.TextColor3 = Color3.fromRGB(88, 101, 242)
    ValueLabel.TextSize = 13
    ValueLabel.TextXAlignment = Enum.TextXAlignment.Right
    
    local SliderBG = Instance.new("Frame", SliderFrame)
    SliderBG.BackgroundColor3 = Color3.fromRGB(35, 35, 50)
    SliderBG.Position = UDim2.new(0, 12, 0, 30)
    SliderBG.Size = UDim2.new(1, -24, 0, 8)
    Instance.new("UICorner", SliderBG).CornerRadius = UDim.new(1, 0)
    
    local SliderFill = Instance.new("Frame", SliderBG)
    SliderFill.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
    SliderFill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
    Instance.new("UICorner", SliderFill).CornerRadius = UDim.new(1, 0)
    
    local SliderKnob = Instance.new("Frame", SliderFill)
    SliderKnob.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    SliderKnob.AnchorPoint = Vector2.new(0.5, 0.5)
    SliderKnob.Position = UDim2.new(1, 0, 0.5, 0)
    SliderKnob.Size = UDim2.new(0, 14, 0, 14)
    Instance.new("UICorner", SliderKnob).CornerRadius = UDim.new(1, 0)
    
    local ClickArea = Instance.new("TextButton", SliderBG)
    ClickArea.BackgroundTransparency = 1
    ClickArea.Size = UDim2.new(1, 0, 1, 16)
    ClickArea.Position = UDim2.new(0, 0, 0, -8)
    ClickArea.Text = ""
    ClickArea.ZIndex = 5
    
    local dragging = false
    
    ClickArea.MouseButton1Down:Connect(function()
        dragging = true
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    RunService.RenderStepped:Connect(function()
        if dragging then
            local mouse = UserInputService:GetMouseLocation()
            local framePos = SliderBG.AbsolutePosition.X
            local frameSize = SliderBG.AbsoluteSize.X
            local pct = math.clamp((mouse.X - framePos) / frameSize, 0, 1)
            local value = math.floor(min + (max - min) * pct)
            
            SliderFill.Size = UDim2.new(pct, 0, 1, 0)
            ValueLabel.Text = tostring(value)
            if callback then callback(value) end
        end
    end)
end

local function CreateButton(parent, text, callback)
    local order = #parent:GetChildren()
    
    local BtnFrame = Instance.new("TextButton", parent)
    BtnFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
    BtnFrame.BorderSizePixel = 0
    BtnFrame.Size = UDim2.new(1, 0, 0, 34)
    BtnFrame.Font = Enum.Font.GothamBold
    BtnFrame.Text = text
    BtnFrame.TextColor3 = Color3.fromRGB(200, 200, 215)
    BtnFrame.TextSize = 13
    BtnFrame.LayoutOrder = order
    BtnFrame.AutoButtonColor = false
    Instance.new("UICorner", BtnFrame).CornerRadius = UDim.new(0, 8)
    
    local BtnStroke = Instance.new("UIStroke", BtnFrame)
    BtnStroke.Color = Color3.fromRGB(50, 50, 70)
    BtnStroke.Thickness = 1
    
    BtnFrame.MouseEnter:Connect(function()
        TweenService:Create(BtnFrame, TweenInfo.new(0.15), {
            BackgroundColor3 = Color3.fromRGB(88, 101, 242)
        }):Play()
        TweenService:Create(BtnStroke, TweenInfo.new(0.15), {
            Color = Color3.fromRGB(88, 101, 242)
        }):Play()
    end)
    
    BtnFrame.MouseLeave:Connect(function()
        TweenService:Create(BtnFrame, TweenInfo.new(0.15), {
            BackgroundColor3 = Color3.fromRGB(30, 30, 50)
        }):Play()
        TweenService:Create(BtnStroke, TweenInfo.new(0.15), {
            Color = Color3.fromRGB(50, 50, 70)
        }):Play()
    end)
    
    BtnFrame.MouseButton1Click:Connect(function()
        if callback then callback() end
    end)
end

local function CreateLabel(parent, text)
    local order = #parent:GetChildren()
    local lbl = Instance.new("TextLabel", parent)
    lbl.BackgroundTransparency = 1
    lbl.Size = UDim2.new(1, 0, 0, 20)
    lbl.Font = Enum.Font.GothamBold
    lbl.Text = "â”€â”€â”€ " .. text .. " â”€â”€â”€"
    lbl.TextColor3 = Color3.fromRGB(88, 101, 242)
    lbl.TextSize = 11
    lbl.LayoutOrder = order
end

local function CreatePlayerDropdown(parent, text, callback)
    local order = #parent:GetChildren()
    
    local DropFrame = Instance.new("Frame", parent)
    DropFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    DropFrame.BorderSizePixel = 0
    DropFrame.Size = UDim2.new(1, 0, 0, 38)
    DropFrame.LayoutOrder = order
    Instance.new("UICorner", DropFrame).CornerRadius = UDim.new(0, 8)
    
    local DropLabel = Instance.new("TextLabel", DropFrame)
    DropLabel.BackgroundTransparency = 1
    DropLabel.Position = UDim2.new(0, 12, 0, 0)
    DropLabel.Size = UDim2.new(0.4, 0, 1, 0)
    DropLabel.Font = Enum.Font.Gotham
    DropLabel.Text = text
    DropLabel.TextColor3 = Color3.fromRGB(200, 200, 215)
    DropLabel.TextSize = 13
    DropLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    local DropBtn = Instance.new("TextButton", DropFrame)
    DropBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 55)
    DropBtn.Position = UDim2.new(0.42, 0, 0.13, 0)
    DropBtn.Size = UDim2.new(0.55, 0, 0.74, 0)
    DropBtn.Font = Enum.Font.Gotham
    DropBtn.Text = "Select Player â–¼"
    DropBtn.TextColor3 = Color3.fromRGB(150, 150, 170)
    DropBtn.TextSize = 11
    DropBtn.AutoButtonColor = false
    Instance.new("UICorner", DropBtn).CornerRadius = UDim.new(0, 6)
    
    local DropList = Instance.new("Frame", DropFrame)
    DropList.BackgroundColor3 = Color3.fromRGB(20, 20, 35)
    DropList.Position = UDim2.new(0.42, 0, 1, 2)
    DropList.Size = UDim2.new(0.55, 0, 0, 0)
    DropList.ClipsDescendants = true
    DropList.ZIndex = 10
    DropList.Visible = false
    Instance.new("UICorner", DropList).CornerRadius = UDim.new(0, 6)
    
    local DropListLayout = Instance.new("UIListLayout", DropList)
    DropListLayout.Padding = UDim.new(0, 1)
    
    local isOpen = false
    
    local function RefreshPlayers()
        for _, c in pairs(DropList:GetChildren()) do
            if c:IsA("TextButton") then c:Destroy() end
        end
        local count = 0
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= player1 then
                count = count + 1
                local PlrBtn = Instance.new("TextButton", DropList)
                PlrBtn.BackgroundColor3 = Color3.fromRGB(28, 28, 45)
                PlrBtn.BorderSizePixel = 0
                PlrBtn.Size = UDim2.new(1, 0, 0, 26)
                PlrBtn.Font = Enum.Font.Gotham
                PlrBtn.Text = plr.Name
                PlrBtn.TextColor3 = Color3.fromRGB(200, 200, 215)
                PlrBtn.TextSize = 11
                PlrBtn.AutoButtonColor = false
                PlrBtn.ZIndex = 11
                
                PlrBtn.MouseEnter:Connect(function()
                    PlrBtn.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
                end)
                PlrBtn.MouseLeave:Connect(function()
                    PlrBtn.BackgroundColor3 = Color3.fromRGB(28, 28, 45)
                end)
                PlrBtn.MouseButton1Click:Connect(function()
                    DropBtn.Text = plr.Name
                    isOpen = false
                    TweenService:Create(DropList, TweenInfo.new(0.2), {
                        Size = UDim2.new(0.55, 0, 0, 0)
                    }):Play()
                    task.delay(0.2, function() DropList.Visible = false end)
                    if callback then callback(plr) end
                end)
            end
        end
        DropList.CanvasSize = UDim2.new(0, 0, 0, count * 27)
    end
    
    DropBtn.MouseButton1Click:Connect(function()
        isOpen = not isOpen
        if isOpen then
            RefreshPlayers()
            DropList.Visible = true
            local count = math.min(#Players:GetPlayers() - 1, 5)
            TweenService:Create(DropList, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
                Size = UDim2.new(0.55, 0, 0, math.max(count * 27, 27))
            }):Play()
        else
            TweenService:Create(DropList, TweenInfo.new(0.2), {
                Size = UDim2.new(0.55, 0, 0, 0)
            }):Play()
            task.delay(0.2, function() DropList.Visible = false end)
        end
    end)
end

