    FeatureStates.Noclip = enabled
    Notify(enabled and "ğŸ‘» Noclip" or "ğŸ‘» Noclip", enabled and "Walking through walls" or "Noclip disabled", 2,
        enabled and Color3.fromRGB(87, 242, 135) or Color3.fromRGB(242, 87, 87))
end)

