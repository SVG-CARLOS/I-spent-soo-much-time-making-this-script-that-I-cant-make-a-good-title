    Notify(enabled and "âœˆï¸ Fly" or "âœˆï¸ Fly", enabled and "Fly mode enabled" or "Fly mode disabled", 2,
        enabled and Color3.fromRGB(87, 242, 135) or Color3.fromRGB(242, 87, 87))
end)

