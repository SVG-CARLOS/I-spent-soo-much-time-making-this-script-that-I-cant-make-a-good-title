-- PLAYER TAB
local PlayerPage = Pages["Player"]
CreateLabel(PlayerPage, "PLAYER ACTIONS")

local FlingToggle = CreateToggle(PlayerPage, "Fling Mode", false, "F", function(enabled)
    FeatureStates.Fling = enabled
    Notify("ğŸ’¥ Fling", enabled and "Touch players to fling them!" or "Fling disabled", 3,
        enabled and Color3.fromRGB(242, 175, 87) or Color3.fromRGB(242, 87, 87))
end)

local selectedPlayer = nil

CreatePlayerDropdown(PlayerPage, "Target Player", function(plr)
    selectedPlayer = plr
    Notify("ğŸ¯ Target", "Selected: " .. plr.Name, 2)
end)

CreateButton(PlayerPage, "ğŸ“ Teleport to Player", function()
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        if workspace:FindFirstChild("VirtualBody") then
            workspace.VirtualBody:SetPrimaryPartCFrame(selectedPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0))
            workspace.VirtualRig:SetPrimaryPartCFrame(selectedPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0))
        end
        Notify("ğŸ“ Teleport", "Teleported to " .. selectedPlayer.Name, 2, Color3.fromRGB(87, 242, 135))
    else
        Notify("âŒ Error", "Select a valid player first!", 2, Color3.fromRGB(242, 87, 87))
    end
end)

CreateButton(PlayerPage, "ğŸ§² Bring Player (Fling Towards You)", function()
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        -- Fling self towards the player to push them
        Notify("ğŸ§² Bring", "Flinging towards " .. selectedPlayer.Name, 2, Color3.fromRGB(242, 175, 87))
    else
        Notify("âŒ Error", "Select a valid player first!", 2, Color3.fromRGB(242, 87, 87))
    end
end)

local OrbitToggle = CreateToggle(PlayerPage, "Orbit Player", false, nil, function(enabled)
    FeatureStates.Orbit = enabled
    FeatureStates.OrbitTarget = selectedPlayer
    Notify("ğŸŒ€ Orbit", enabled and ("Orbiting " .. (selectedPlayer and selectedPlayer.Name or "nobody")) or "Orbit disabled", 2)
end)

CreateSlider(PlayerPage, "Orbit Radius", 5, 30, 10, function(val)
    FeatureStates.OrbitRadius = val
end)

